@model MOBYNew.Models.Item
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Item Detail</h2>

<ul>
    
    <li>Genre: @Model.Genre.genreName</li>
    
    <li>Release Date: @Model.ReleaseDate.ToLongDateString()</li>
    
    <li>Price: @Model.Price.ToString()</li>
    
    <li>Number in Stock: @Model.QtyInStock</li>
    
</ul>

